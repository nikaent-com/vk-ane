<extension xmlns="http://ns.adobe.com/air/extension/4.0">
        <id>com.nikaent.ane.vk</id>
        <versionNumber>1</versionNumber>
        <platforms>
                <platform name="Android-ARM">
                        <applicationDeployment>
                                <nativeLibrary>libane.jar</nativeLibrary>
                                <initializer>com.nikaent.ane.vk.AneVk</initializer>
                                <finalizer>com.nikaent.ane.vk.AneVk</finalizer>
                        </applicationDeployment>
                </platform>
                <platform name="Android-x86">
                        <applicationDeployment>
                                <nativeLibrary>libane.jar</nativeLibrary>
                                <initializer>com.nikaent.ane.vk.AneVk</initializer>
                                <finalizer>com.nikaent.ane.vk.AneVk</finalizer>
                        </applicationDeployment>
                </platform>
                <platform name="iPhone-ARM">
                        <applicationDeployment>
                                <nativeLibrary>libane.a</nativeLibrary>
                                <initializer>VolumeExtensionInitializer</initializer>
                        </applicationDeployment>
                </platform>
                <platform name="iPhone-x86">
                        <applicationDeployment>
                                <nativeLibrary>libane.a</nativeLibrary>
                                <initializer>VolumeExtensionInitializer</initializer>
                        </applicationDeployment>
                </platform>
                <platform name="default"> 
                    <applicationDeployment/>
                </platform>
        </platforms>
</extension>